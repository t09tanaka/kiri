{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:window:allow-destroy",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-ask",
    "store:default",
    "mcp-bridge:default",
    "opener:default"
  ]
}
